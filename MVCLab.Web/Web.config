  </entityFramework>
  <connectionStrings>
    <add name="CrmContext" connectionString="data source=(LocalDb)\MSSQLLocalDB;initial catalog=MvcLab.Models.CrmContext;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.webServer>
